import math
print("Введите коэффициенты для уравнения")
print("ax^2 + bx + c = 0:")
a = float(input())
b = float(input())
c = float(input())
D = b ** 2 - 4 * a * c
if D > 0:
    X1 = (-(b) + math.sqrt(D)) / (2 * a)
    X2 = (-(b) - math.sqrt(D)) / (2 * a)
    print(X1, X2)
elif D = 0:
    print(-(b) / (2 * a))
else:
    print("Не имеет корней")
